---
title: home page
layout: base
eleventyNavigation:
  key: home
  order: 1
pagination:
  data: collections.post
  size: 12
  alias: posts
---

<nav>
  {% set navPages = collections.all | eleventyNavigation %}
  <ul class="list-inside list-disc">
    <li>info</li>
    <ul class="list-inside list-disc pl-4">
      <li>description</li>
      <li>some links</li>
    </ul>
    <li>all posts</li>
    <ul class="list-inside list-disc pl-4">
      {% for post in posts %}
      <li>
        <a href="{{post.url}}" hx-boost="true">{{ post.data.title }}</a>
      </li>
      {% endfor %}
    </ul>
  </ul>
</nav>